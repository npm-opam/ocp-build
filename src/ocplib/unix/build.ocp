
begin library "ocplib-unix"
  files = [
    "minUnix.ml" "minUnix_c.c"
    "onlyUnix.ml" "onlyUnix_c.c"
    "onlyWin32.ml" "onlyWin32_c.c"
  ]
  if system = "win64" then
    link = [ "-cclib"; "user32.lib" ]
  else
(* We require "Unix", but do not use it directly !!! *)
    requires = [ "unix" ]
end

begin library "no-unix"
  files = [ "unix.ml" ]
end
